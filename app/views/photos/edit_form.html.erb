<h1>Edit a photo</h1>

<form action= <%= "http://localhost:3000/update_photo/" + @f.id.to_s %>>
  <div>
    <label for="photo_caption">Caption:</label>
    <input id="photo_caption" type="text" name="the_caption" value="<%= @f.caption %>">
  </div>
  <div>
    <label for="photo_image_url">Image URL:</label>
    <input id="photo_image_url" type="text" name="the_source" value="<%= @f.source %>">
  </div>
  <div>
    <button>Update Photo</button>
  </div>
</form>
